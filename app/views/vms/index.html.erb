<h1>Listing vms</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Uuid</th>
    <th>Mem</th>
    <th>Cpus</th>
    <th>Vncdisplay</th>
    <th>Diskname</th>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @vms.each do |vm| %>
  <tr>
    <td><%= vm.name %></td>
    <td><%= vm.uuid %></td>
    <td><%= vm.mem %></td>
    <td><%= vm.cpus %></td>
    <td><%= vm.vncdisplay %></td>
    <td><%= vm.diskname %></td>
    <td><%= vm.type %></td>
    <td><%= link_to 'Show', vm %></td>
    <td><%= link_to 'Edit', edit_vm_path(vm) %></td>
    <td><%= link_to 'Destroy', vm, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vm', new_vm_path %>
